{
  "root": true,
  "plugins": [
    "prettierx"
  ],
  "parserOptions": {
    "ecmaVersion": 2020
  },
  "env": {
    "es6": true,
    "browser": false,
    "node": true
  },
  "extends": [
    "standard",
    "plugin:prettierx/standardx"
  ],
  "rules": {
    "prettierx/options": "error",
    "no-unused-vars": ["error", {"args": "all", "argsIgnorePattern": "^_"}]
  }
}
